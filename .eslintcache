ost?u=3ea0d06a53629b8c41e72ed2d&amp;id=cc15af59c5';\n\n// copied from here https://github.com/revolunet/react-mailchimp-subscribe/blob/b22aac36b6ad2cd290b30490e573efb0744925e0/demo/src/index.js#L8\nfunction SubscribeForm() {\n  return (\n    <MailchimpSubscribe\n      url={url}\n      render={({ subscribe, status, message }) => (\n        <CustomForm\n          status={status}\n          message={message}\n          onValidated={(formData) => subscribe(formData)}\n        />\n      )}\n    />\n  );\n}\n\nconst CustomForm = ({ status, message, onValidated }) => {\n  const [email, setEmail] = useState('');\n  const [type, setType] = useState('Mentor');\n\n  const submit = () => email\n    && email\n    && email.indexOf('@') > -1\n    && onValidated({\n      EMAIL: email,\n      TYPE: type,\n    });\n\n  return (\n    <>\n      <h1\n        id=\"newsletter\"\n        style={{\n          maxWidth: 300, textAlign: 'center', margin: 60, marginBottom: 0,\n        }}\n      >\n        We are on our way\n      </h1>\n      <p style={{\n        maxWidth: 300, textAlign: 'center', margin: 60, marginTop: 10,\n      }}\n      >\n        Fill this interest form and be the first to know about our launch!\n      </p>\n      <div\n        className=\"subscribe-form\"\n      >\n        {/* {status === 'sending' && <div style={{ color: 'blue' }}>sending...</div>} */}\n        {status === 'error' && (\n        <div\n          style={{ color: 'black' }}\n          dangerouslySetInnerHTML={{ __html: message }}\n        />\n        )}\n        {status === 'success' && (\n        <div\n          style={{ color: 'black' }}\n          dangerouslySetInnerHTML={{ __html: message }}\n        />\n        )}\n        {status ? null\n          : (\n            <>\n              <input\n                onChange={(e) => setEmail(e.target.value)}\n                style={{ width: '80%' }}\n                type=\"text\"\n                placeholder=\"Your email\"\n              />\n              <p style={{ marginTop: 20, textAlign: 'center' }}>What would you like to be on Troosh?</p>\n              <div onChange={(e) => setType(e.target.value)} className=\"account-type\">\n                <div className=\"subscribe-radio\">\n                  <input type=\"radio\" value=\"Mentee\" name=\"type\" id=\"mentee-input\" />\n                  <label htmlFor=\"mentee-input\">Mentee</label>\n                </div>\n                <div className=\"subscribe-radio\">\n                  <input type=\"radio\" value=\"Mentor\" name=\"type\" id=\"mentor-input\" />\n                  <label htmlFor=\"mentor-input\">Mentor</label>\n                </div>\n              </div>\n              <button onClick={submit} type=\"submit\">\n                Subscribe\n              </button>\n            </>\n          )}\n\n      </div>\n    </>\n  );\n};\n\nexport default SubscribeForm;\n","/Users/dimitrisstefanakis/dev/influencity-web/src/flat/PricingCompact/PricingCompact.js",[],"/Users/dimitrisstefanakis/dev/influencity-web/src/flat/Footer/Footer.js",[],"/Users/dimitrisstefanakis/dev/influencity-web/src/flat/Faq/Faq.js",[],"/Users/dimitrisstefanakis/dev/influencity-web/src/flat/BetweenHeader/BetweenHeader.js",[],"/Users/dimitrisstefanakis/dev/influencity-web/src/flat/WhatWeOffer/ForMentors.js",[],"/Users/dimitrisstefanakis/dev/influencity-web/src/flat/WhatWeOffer/ForMentees.js",[],"/Users/dimitrisstefanakis/dev/influencity-web/src/flat/Faq/items.js",[],"/Users/dimitrisstefanakis/dev/influencity-web/src/flat/ChainedBoxes/index.js",[],"/Users/dimitrisstefanakis/dev/influencity-web/src/flat/PricingBox/index.js",[],"/Users/dimitrisstefanakis/dev/influencity-web/src/flat/Burger/index.js",[],"/Users/dimitrisstefanakis/dev/influencity-web/src/flat/FaqItem/index.js",[],"/Users/dimitrisstefanakis/dev/influencity-web/src/flat/ProjectPricingBox/index.js",[],"/Users/dimitrisstefanakis/dev/influencity-web/src/flat/Blob/index.js",[],"/Users/dimitrisstefanakis/dev/influencity-web/src/flat/Chained
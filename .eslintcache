&& email.indexOf('@') > -1\n    && onValidated({\n      EMAIL: email,\n      TYPE: type,\n    });\n\n  return (\n    <>\n      <h1 style={{\n        maxWidth: 300, textAlign: 'center', margin: 60, marginBottom: 0,\n      }}\n      >\n        We are on our way\n      </h1>\n      <p style={{\n        maxWidth: 300, textAlign: 'center', margin: 60, marginTop: 10,\n      }}\n      >\n        Fill this interest form and be the first to know about our launch!\n      </p>\n      <div\n        className=\"subscribe-form\"\n      >\n        {/* {status === 'sending' && <div style={{ color: 'blue' }}>sending...</div>} */}\n        {status === 'error' && (\n        <div\n          style={{ color: 'black' }}\n          dangerouslySetInnerHTML={{ __html: message }}\n        />\n        )}\n        {status === 'success' && (\n        <div\n          style={{ color: 'black' }}\n          dangerouslySetInnerHTML={{ __html: message }}\n        />\n        )}\n        {status ? null\n          : (\n            <>\n              <input\n                onChange={(e) => setEmail(e.target.value)}\n                style={{ width: '80%' }}\n                type=\"text\"\n                placeholder=\"Your email\"\n              />\n              <p style={{ marginTop: 20, textAlign: 'center' }}>What would you like to be on Troosh?</p>\n              <div onChange={(e) => setType(e.target.value)} className=\"account-type\">\n                <div className=\"subscribe-radio\">\n                  <input type=\"radio\" value=\"Mentee\" name=\"type\" id=\"mentee-input\" />\n                  <label htmlFor=\"mentee-input\">Mentee</label>\n                </div>\n                <div className=\"subscribe-radio\">\n                  <input type=\"radio\" value=\"Mentor\" name=\"type\" id=\"mentor-input\" />\n                  <label htmlFor=\"mentor-input\">Mentor</label>\n                </div>\n              </div>\n              <button onClick={submit} type=\"submit\">\n                Subscribe\n              </button>\n            </>\n          )}\n\n      </div>\n    </>\n  );\n};\n\nexport default SubscribeForm;\n","/Users/dimitris/Desktop/dev/troosh/influencity-web/src/flat/Header/Header.js",[],"/Users/dimitris/Desktop/dev/troosh/influencity-web/src/flat/Pricing/Pricing.js",[],"/Users/dimitris/Desktop/dev/troosh/influencity-web/src/flat/WhatWeOffer/ForMentors.js",[],"/Users/dimitris/Desktop/dev/troosh/influencity-web/src/flat/WhatWeOffer/ForMentees.js",[],"/Users/dimitris/Desktop/dev/troosh/influencity-web/src/flat/ChainedBoxes/index.js",[],"/Users/dimitris/Desktop/dev/troosh/influencity-web/src/flat/PricingBox/index.js",[],"/Users/dimitris/Desktop/dev/troosh/influencity-web/src/flat/ChainedBoxes/ChainedBoxes.js",[],"/Users/dimitris/Desktop/dev/troosh/influencity-web/src/flat/PricingBox/PricingBox.js",[],"/Users/dimitris/Desktop/dev/troosh/influencity-web/src/flat/PricingBox/FreePricingBox.js",[],"/Users/dimitris/Desktop/dev/troosh/influencity-web/src/flat/PricingBox/BasicPricingBox.js",[],"/Users/dimitris/Desktop/dev/troosh/influencity-web/src/flat/PricingBox/PremiumPricingBox.js",[],{"ruleId":"116","replacedBy":"117"},{"ruleId":"118","replacedBy":"119"},{"ruleId":"120","replacedBy":"121"},{"ruleId":"122","replacedBy":"123"},{"ruleId":"124","replacedBy":"125"},{"ruleId":"126","replacedBy":"127"},{"ruleId":"128","severity":1,"message":"129","line":57,"column":11,"nodeType":"130","endLine":57,"endColumn":56},{"ruleId":"128","severity":1,"message":"129","line":63,"column":11,"nodeType":"130","endLine":63,"endColumn":56},"lines-around-directive",["131"],"no-spaced-func",["132"],"global-require",[],"no-buffer-constructor",[],"no-new-require",[],"no-path-concat",[],"react/no-danger","Dangerous property 'dangerouslySetInnerHTML' found","JSXAttribute","padding-line-between-statements","func-call-spacing"]